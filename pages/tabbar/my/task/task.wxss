page {
  background: #efefef;
  padding-bottom: 25rpx;
}

.limit {
  background-color: #fff;
  position: relative;
  box-shadow: #644ceb 0 0 15rpx;
  min-height: 200rpx;
  margin-bottom: 45rpx;
  z-index: 33;
}

.limit-bg {
  top: 0;
  left: 0;
  position: absolute;
  width: 750rpx;
  z-index: -1;
}

.limit-title {
  padding-top: 79rpx;
  text-align: center;
  font-weight: bold;
  height: 126rpx;
  color: #fff;
}

.limit-list {
  padding: 25rpx 25rpx 10rpx 25rpx;
  overflow: hidden;
  position: relative;
  color: #fff;
}

.limit-list::before {
  content: "";
  opacity: .8;
  position: absolute;
  height: 100%;
  width: 100%;
  top: 0;
  z-index: 33;
  left: 0;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}

.limit-list .limit-item {
  position: relative;
  border-radius: 15rpx;
  background: #644ceb;
  padding: 10rpx;
  margin-bottom: 15rpx;
}

.limit-list van-icon {
  vertical-align: middle;
  font-size: 32rpx;
}

.limit-list .limit-item-p {
  padding: 15rpx 0;
}

.tasks {
  background: #f6feff;
  margin-bottom: 25rpx;
}

.tasks-daily {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  box-shadow: #4abdff 0 0 4rpx;
  padding: 0 25rpx;
}

.tasks-daily .tasks-title {
  margin-bottom: 30rpx;
}

.tasks-daily .limit-list {
  width: 338rpx;
  padding: 0;
}

.tasks-daily .limit-reward {
  position: unset;
  padding-bottom: 8rpx;
}

.tasks-daily .limit-item {
  background: #4cbdfd;
  margin-bottom: 20rpx;
}

.tasks-daily .tasks-title .t-t-bg {
  width: 650rpx;
}

.tasks-weekly {
  box-shadow: #1fac7d 0 0 4rpx;
}

.tasks-weekly .limit-item {
  background: #1fac7d;
}

.tasks-resident {
  box-shadow: #e48c52 0 0 4rpx;
}

.tasks-resident .limit-item {
  background: #c37c4d;
}

.tasks-title {
  position: relative;
  text-align: center;
  font-weight: bold;
  padding: 25rpx;
  width: 100%;
  color: #1989fa;
  margin-bottom: 15rpx;
  font-size: 30rpx;
}

.tasks-title .t-t-bg {
  width: 700rpx;
  position: absolute;
  margin: 0 auto;
  top: 28rpx;
}

.tasks-title image {
  width: 255rpx;
}

.limit-item-title {
  font-size: 30rpx;
  padding-bottom: 8rpx;
}

.limit-item-p {
  width: 80%;
}

@keyframes arrow {
  0% {
    right: 8rpx;
  }
  50% {
    right: 16rpx;
  }
  100% {
    right: 8rpx;
  }
}

.icon-arrow {
  position: absolute;
  right: 8rpx;
  bottom: 14rpx;
}

.icon-arrow._done {
  animation: arrow 1s infinite;
}

.limit-time {
  height: 385rpx;
  overflow: hidden;
}

.limit-time.limit-time-all {
  height: auto;
}

.limit-time.limit-time-all .limit-more {
  display: none;
}

.limit-more {
  width: 750rpx;
  padding: 45rpx 0 10rpx 0;
  position: absolute;
  bottom: 0;
  z-index: 33;
  text-align: center;
  font-weight: bold;
  background-image: linear-gradient(to bottom, rgba(255, 255, 255, 0), rgba(255, 255, 255, 0.9), white);
}

.tasks-vip {
  opacity: .3;
  border: 1px solid #e452ff;
}

._vip .limit-item {
  background: linear-gradient(24deg, #e452ff, #d37cf7, #ff3f3f);
}

.bg-ywc {
  position: absolute;
  z-index: 333;
  top: 0;
  right: 0;
}

.bg-ywc image {
  width: 205rpx;
  height: 159rpx;
}
