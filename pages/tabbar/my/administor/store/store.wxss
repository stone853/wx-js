.container {
	position: relative;
	padding-top: 20rpx;
}
.container textarea{
  padding-top: 4px;
  min-height: 48px;
}
.eckes-form .section{
	margin: 0 15rpx 30rpx 15rpx;
	background-color: #fff;
	border-radius: 10rpx;
	padding: 30rpx 25rpx 1rpx 25rpx;
	display: block;
	position: relative;
}
.top-bg{
	width: 100%;
	height: 170rpx;
	position: absolute;
	top: 0;
	left: 0;
}
.register-title{
	color: #2B6CDE;
	font-size: 28rpx;
	height: 65rpx;
	text-align: center;
	border-bottom: 1px solid #e5e5e5;
}
.secret{
	font-size: 26rpx;
	text-align: center;
	color: #F6A03B;
	padding: 20rpx 0;
}
.eckes-btn{
	height: 54rpx;
	line-height: 53rpx;
	text-align: center;
	color: #fff;
	padding: 0 11rpx;
	border-radius: 8rpx;
  background: linear-gradient(-60deg, #35E0CE, #08C5B1);
}
.parentClass .eckes-input{
	margin-bottom: 18rpx;
}
.eckes-form-btn{
	padding-bottom: 50rpx;
}
.eckes-submit{
	width: 720rpx;
	line-height: 88rpx;
	background: #0BB7A5;
	border-radius: 10rpx;
	color: #fff;
}
textarea{
	max-height: 320rpx;
}
.item{
	position: relative;
	width: 720rpx;
	margin-left:15rpx;
	border-radius: 5rpx;
	box-shadow: #ddd 0 0 15rpx;
	padding: 10rpx 110rpx 10rpx 10rpx;
	margin-bottom: 20rpx;
}
.item>view{
	margin-bottom: 10rpx;
	color: #666;
}
.item-logo{
	position: absolute;
	right: 10rpx;
	top: 10rpx;
}
.item-logo image{
	width: 100rpx;
	height: 100rpx;
}
.item-bg image{
	width: 690rpx;
	height: 345rpx;
}
.item-subtitle{
	color: #999;
}
.item-btn{
	position: relative;
}
.item-btn>view{
	display: inline-block;
	margin-right: 50rpx;
}