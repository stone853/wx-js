.eckes-scroll{
  background:#f7faff;
  color: #2C6CDE;
  font-weight: bold;
  position: relative;
}
.eckes-scroll::before{
  position: absolute;
  z-index:0;
  background: #2c6cde;
  height: 1px;
  width: 100%;
  content:" ";
  left:0;
  bottom: 0;
}
.eckes-scroll .scroll-view{
  border-bottom:0;
}
.eckes-scroll .scroll-view-item{
  position: relative;
  z-index: 23;
}
.eckes-scroll .scroll-view-item.action {
  background-color: #fff;
  border: 1px solid #2c6cde;
  border-bottom-color: #fff;
  color: #2C6CDE;
}
.eckes-scroll .scroll-view-item.action::before{
  display: none
}
