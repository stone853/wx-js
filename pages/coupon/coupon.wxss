.box {
  width: 100rpx;
  height: 100rpx;
  margin: 10rpx auto;
  position: relative;
}

.box view {
  box-sizing: content-box;
  position: absolute;
  top: 0;
  left: 0;
  border-radius: 50%;
  height: 80rpx;
  width: 80rpx;
  -webkit-transition: all 0.2s ease-in-out;
  -moz-transition: all 0.2s ease-in-out;
}

.box .bg {
  border: #fedbe3 10rpx solid;
}

.box .bg2 {
  border: #fedbe3 10rpx solid;
  clip: rect(0, 50rpx, 100rpx, 0);
}

.box .rount {
  border: #fa265f 10rpx solid;
  clip: rect(0, 50rpx, 100rpx, 0);
  -webkit-transform: rotate(0deg);
  display: inline;
}

.box .rount2 {
  border: #fa265f 10rpx solid;
  clip: rect(0, 100rpx, 100rpx, 50rpx);
  -webkit-transform: rotate(72deg);
   display: none;
 
}

.box .num {
  font-size:24rpx;
  text-align: center;
  color: #fa265f;
  top: 20rpx;
  left: 10rpx;
  z-index: 9;
}

