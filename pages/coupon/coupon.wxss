.chart {
  width: 100rpx;
  height: 100rpx;
  float: left;
  position: relative;
  line-height: 100rpx;
  text-align: center;
}

.chart::before {
  content: "";
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  border: 8rpx solid #fedbe3;
  border-radius: 50%;
  z-index: 1;
}

.chart::after {
  content: "";
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  border: 8rpx solid #fa265f;
  border-radius: 50%;
  clip: rect(0, auto, auto, 50rpx);
  z-index: 2;
}

.chart .chart-progress {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  border: 8rpx solid #fa265f;
  border-radius: 50%;
  clip: rect(0 auto, auto, 50rpx);
  z-index: 3;
}

.chart-text {
  font-size: 22rpx;
  color: #fa265f;
  text-align: center;
  line-height: 30rpx;
  margin-top: 20rpx;
}

