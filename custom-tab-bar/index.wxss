.tab-content{
  height:146rpx;
}
.tab-bar {
  height: 98rpx;
  background: #fff;
  display: flex;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  /* padding-bottom: env(safe-area-inset-bottom); */
  z-index: 999;
  box-sizing: content-box;
}

.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  flex-direction: column;
}

.tab-bar-item .item-image {
  width: 44rpx;
  height: 44rpx;
}

.tab-bar-item .item-view {
  font-size: 28rpx;
  line-height: 24rpx;
  margin: 11rpx;
}

.scanCode {
  height: 90rpx;
  width: 90rpx;
  border-radius: 50%;
  background: #EEF1F8;
  padding:9rpx;
}
.scanCode-bg{
  background: #5383F9;
  width: 100%;
  height:100%;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.scanCode-bg .item-image{
  width: 46rpx;
  height: 46rpx;
}
