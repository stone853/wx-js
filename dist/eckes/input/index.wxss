.eckes-input{
    display: flex;
    width: 100%;
    align-items:flex-end;
    margin-bottom: 28rpx;
}
.eckes-nolabel{
    margin-bottom: 0;
    min-height: 69rpx;
}
.eckes-border{
    border-bottom: 1px solid #e5e5e5;
}
.eckes-input-box{
    flex: 1;
    padding-bottom: 10rpx;
}

.eckes-input-label{
    color: #333;
    font-size: 28rpx;
    margin-bottom: 20rpx;
}
.eckes-input.eckes-required .eckes-input-label text{
    position: relative;
}
.eckes-input.eckes-required .eckes-input-label text::after{
    content: "*";
    position: absolute;
    color: #FF0600;
    right: -20rpx;
    top: 0;
    font-size: 32rpx;
}
.placeholder{
    color: #b2b2b2;
    font-size: 26rpx;
}
.eckes-input-item{
    font-size: 26rpx;
    color: #666;
}
.eckes-nolabel .eckes-input-item
.eckes-nolabel .placeholder{
    font-size: 28rpx;
}
.eckes-nolabel .picker-icon{
    bottom: 16rpx;
}

radio-group radio
{
    margin-left: 16rpx;
    width: 0;
    height: 0;
    overflow: hidden;
}
radio-group label{
    margin-right: 44rpx;
    font-size: 28rpx;
    color: #666;
    padding-left: 20px;
    background-repeat: no-repeat;
    background-position: 0 center;
    background-image: url(data:image/png;base64,);
    background-size: 19px 19px;
}
radio-group label[aria-checked="true"]{
    background-image: url(data:image/png;base64,);
}

checkbox-group{
    display: flex;
    flex-wrap: wrap;
    margin:0 -10rpx;
}
checkbox-group checkbox{
    width: 0;
    height: 0;
    overflow: hidden;
}
checkbox-group label{
    margin: 0 9rpx;
    width: 210rpx;
    background: #F7F7F7;
    text-align: center;
    line-height: 69rpx;
    color: #333;
    font-size: 26rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    margin-bottom: 19rpx;
    position: relative;
    border-radius: 6rpx;
    border: 1px solid #F7F7F7;
}
checkbox-group label[aria-checked="true"]{
    border-color: #0BB7A5;
    background-color: #E3FDFA;
    color: #0BB7A5;
}
checkbox-group label[aria-checked="true"]::before{
    content: "";
    position: absolute;
    right: 0;
    bottom: 0;
    line-height: normal;
    background:url(data:image/png;base64,) no-repeat;
    width: 30rpx;
    height: 30rpx;
    background-size: 30rpx 30rpx;
}
textarea{
    width: 100%;
    height: auto;
}
.eckes-input-button{
    font-size: 26rpx;
    padding-left: 20rpx;
    padding-bottom: 10rpx;
}
.picker{
    position: relative;
}
.picker-icon{
    position: absolute;
    right: 5px;
    bottom: 7rpx;
    width: 19rpx;
    height: 10rpx;
    background: url(data:image/png;base64,) no-repeat center center;
    background-size: 19rpx 10rpx;
}